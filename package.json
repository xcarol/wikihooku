{
  "name": "wikihooku",
  "version": "0.1.0",
  "description": "Frontend of wikihooku project",
  "private": true,
  "author": "WikiHooku Team",
  "contributors": [
    "xcarolr@gmail.com"
  ],
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "dev": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service serve",
    "build": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build && yarn add-commit",
    "lint": "vue-cli-service lint",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "lint:fix": "vue-cli-service lint --fix",
    "add-commit": "git log | head -n 1 | sed 's/commit //' > dist/commit.html"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "core-js": "^3.30.2",
    "dayjs": "^1.11.7",
    "express": "^4.18.2",
    "infobox-parser": "^3.6.2",
    "register-service-worker": "^1.7.2",
    "vee-validate": "^4.9.5",
    "vite": "^4.3.9",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-recaptcha": "^3.0.0-alpha.5",
    "vue-router": "^4.2.2",
    "vuedraggable": "^2.24.3",
    "vuetify": "^3.3.2",
    "vuex-persist": "^3.1.3",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-airbnb": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-vue": "^9.14.1",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "eslint": "^8.41.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "husky": "^8.0.3",
    "sass": "^1.62.1",
    "vue-cli-plugin-axios": "^0.0.4",
    "vue-cli-plugin-i18n": "^2.3.2",
    "vue-cli-plugin-vuetify": "^2.5.8",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.84.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "vue-cli-service lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
